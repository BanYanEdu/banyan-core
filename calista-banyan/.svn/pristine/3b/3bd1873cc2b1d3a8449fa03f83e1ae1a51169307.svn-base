package com.inet.calista.common.provider;

import com.inet.xportal.nosql.web.provider.CloudConfigAbstraction;
import com.inet.xportal.nosql.web.provider.NoSQLConfigProviderAbstraction;
import com.inet.xportal.web.annotation.XPortalBigData;
import com.inet.xportal.web.context.ApplicationContext;

import javax.inject.Inject;
import javax.inject.Named;

/**
 * NoSqlCalDatabaseProvider.
 *
 * @author Duyen Tang
 * @version $Id: NoSqlCalDatabaseProvider.java 2013-06-12 12:08:50z tttduyen $
 *
 * @since 1.0
 */
@ApplicationContext(context = "CalNoSqlProvider")
@XPortalBigData(context = "cloud_calista_database")
@Named("NoSqlCalDatabaseProvider")
public class NoSqlCalDatabaseProvider extends NoSQLConfigProviderAbstraction {
  @Override
  public boolean contextEnabled() {
    return false;
  }

  @Inject
  public NoSqlCalDatabaseProvider(@ApplicationContext(context ="NoSqlCalConfig") CloudConfigAbstraction cloudCfg) {
    super(cloudCfg);
  }
}
