package com.inet.calista.collatask.dto.project;

import java.util.List;

import com.mongodb.BasicDBObject;

/**
 * AdvanceSearchDto.
 *
 * @author Duyen Tang
 * @version $Id: AdvanceSearchDto.java May 22, 2018 2:58:12 PM tttduyen $
 *
 * @since 1.0
 */
public class AdvanceSearchDto {
  private String code;
  private String name;
  private String desc;
  private long startDateFrom;
  private long startDateTo;
  private long deadlineFrom;
  private long deadlineTo;
  private long completedDateFrom;
  private long completedDateTo;
  private String type;
  private List<String> statuses;
