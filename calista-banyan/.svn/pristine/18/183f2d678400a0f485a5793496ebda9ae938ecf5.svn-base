package com.inet.calista.pos.retail.data;

import com.inet.calista.erp.sale.data.ERPSaleItem;
import org.apache.shiro.util.StringUtils;

/**
 * com.inet.platform.pos.data.
 *
 * @author Phong Tran
 * @version $Id: POSSaleItem.java Jun 07, 2018 16:26 phongtt $
 * @since 1.0
 */
public class POSSaleItem extends ERPSaleItem {

    public POSSaleItem withUuid(String uuid) {
        super.setUuid(uuid);
        return this;
    }

    public POSSaleItem calc() {
        super.calc();
        return this;
    }

    @Override
    public boolean equals(Object obj) {
        if (obj != null && obj instanceof POSSaleItem) {
            if (StringUtils.hasLength(this.getSku()) && this.getSku().equals(((POSSaleItem) obj).getSku())) {
                return true;
            } else {
                return super.equals(obj);
            }
        }

        return super.equals(obj);
    }
}
